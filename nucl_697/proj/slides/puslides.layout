Style "Subtitle"
	Align		Center
	AlignPossible	Center
	LabelType	Static
	LatexType	Command
	Category	Footer
	Margin		Dynamic
	KeepEmpty	1
	InPreamble  1
	LabelFont
		Series Bold
	EndFont
	Labelsep "  "
	LabelString	"Subtitle:"
	LatexName	subtitle
End

Style "Venue"
	CopyStyle	"Subtitle"
	LabelString	"Venue:"
	ParbreakIsNewline 1
	LatexName	venue
End

Style "City"
	CopyStyle	"Subtitle"
	LabelString	"City:"
	ParbreakIsNewline 1
	LatexName	city
End

Style "Authors for Footer"
	CopyStyle	"Subtitle"
	LabelString	"Authors for Footer:"
	ParbreakIsNewline	1
	LatexName	authorsforfooter
End

Style "Shade Quote"
    Category   Content
    LatexType  command
    LatexName  squote

End

InsetLayout "Shade Quote"
	Decoration Classic
	LyxType charstyle
	CustomPars 0
	LatexType Command
	LatexName squote
	LabelString Quote
	ParbreakIsNewline 1
	KeepEmpty 1
  Argument 1
    Mandatory 1
    LabelString "Author"
  EndArgument
End

InsetLayout "Content"
	Decoration Classic
	LyxType charstyle
	CustomPars 1
	FreeSpacing 1
	MultiPar 1
	LatexType Environment
	LatexName content
	LabelString content
	KeepEmpty 1
End

Style "Milestone"
	CopyStyle Paragraph
	LatexType Item_Environment
	LatexName milestone
	Font
		Series Bold
		Color Green
	EndFont
	Argument 1
		Mandatory 0
		LabelString "start number less 1"
	EndArgument
End

Style "Heading"
	CopyStyle Paragraph
	LatexType Command
	LatexName heading
	Font
		Series Bold
	EndFont
End

Style "Chapter"
	CopyStyle Section
	LatexType Command
	LatexName chapter
	Font
		Series Bold
	EndFont
	Argument 1
		Mandatory 1
		LabelString "Long Chapter"
	EndArgument
End

Style "Chapter*"
	CopyStyle Section
	LatexType Command
	LatexName namechapter
	Font
		Series Bold
	EndFont
End

Style "Section*"
	CopyStyle Section
	LatexType Command
	LatexName section
	Font
		Series Bold
	EndFont
End
