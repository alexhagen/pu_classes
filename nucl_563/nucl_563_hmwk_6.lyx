\begin_layout Standard

\series bold
5.22) What is the area required?
\end_layout

\begin_layout Standard
Given 
\begin_inset Formula $J_{s}=0.035\unit{\frac{mA}{cm^{2}}}$
\end_inset

, 
\begin_inset Formula $T=55\unit{\celsius}$
\end_inset

, 
\begin_inset Formula $V_{o}=0.58\unit{V}$
\end_inset

, 
\begin_inset Formula $\eta=0.13$
\end_inset

, needs 
\begin_inset Formula $800\unit{W}$
\end_inset

 at 
\begin_inset Formula $28\unit{V}$
\end_inset

.
\end_layout

\begin_layout Standard
Finding the 
\begin_inset Formula $J_{o}$
\end_inset

 from 
\begin_inset Formula $V_{o}$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
V_{o} & =\frac{kT}{e}\ln\left(\frac{J_{s}}{J_{o}}+1\right)\\
\frac{eV_{o}}{kT} & =\ln\left(\frac{J_{s}}{J_{o}}+1\right)\\
\frac{J_{s}}{\exp\left(\frac{eV_{o}}{kT}\right)-1} & =J_{o}\\
J_{o} & =\frac{0.000035\unit{\frac{A}{cm^{2}}}}{\exp\left(\frac{\left(1\unit{e^{-}}\right)\left(0.58\unit{V}\right)}{\left(8.6\times10^{-5}\unit{\frac{eV}{K}}\right)\left(328.15\unit{K}\right)}\right)-1}\\
 & =1.13\times10^{-13}\unit{\frac{A}{cm^{2}}}
\end{align*}

\end_inset

and then, finding the maximuim power
\begin_inset Formula 
\begin{align*}
P & =\left(J_{s}-J_{o}\left[\exp\left(\frac{eV}{kT}\right)-1\right]\right)V\\
 & =\left(0.000035\unit{\frac{A}{cm^{2}}}-1.13\times10^{-13}\unit{\frac{A}{cm^{2}}}\left[\exp\left(\frac{\left(1\unit{e^{-}}\right)\left(28\unit{V}\right)}{\left(8.6\times10^{-5}\unit{\frac{eV}{K}}\right)\left(328.15\unit{K}\right)}\right)-1\right]\right)\left(28\unit{V}\right)\\
 & =9.8\times10^{-4}\unit{\frac{W}{cm^{2}}}
\end{align*}

\end_inset


\begin_inset Formula 
\[
A=8.2\times10^{5}\unit{cm^{2}}
\]

\end_inset


\end_layout

