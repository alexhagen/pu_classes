\end_layout

\begin_layout Standard
Knowing that 
\begin_inset Formula 
\[
V_{mp}=J_{mp}R_{load}A
\]

\end_inset

from kirchoff's laws, and 
\begin_inset Formula 
\[
J_{mp}=\frac{\frac{eV_{mp}}{kT}\left[J_{s}+J_{0}\right]}{\left(\frac{eV_{mp}}{kT}+1\right)}
\]

\end_inset

we get 
\begin_inset Formula 
\begin{align*}
J_{mp} & =\frac{\frac{eJ_{mp}R_{load}A}{kT}\left[J_{s}+J_{0}\right]}{\left(\frac{eJ_{mp}R_{load}A}{kT}+1\right)}\\
1 & =\frac{\frac{eR_{load}A}{kT}\left[J_{s}+J_{0}\right]}{\left(\frac{eR_{load}A}{kT}+\frac{1}{J_{mp}}\right)}\\
\frac{eR_{load}A}{kT}+\frac{1}{J_{mp}} & =\frac{eR_{load}A}{kT}\left[J_{s}+J_{0}\right]\\
\frac{1}{J_{mp}} & =\frac{eR_{load}A}{kT}\left[J_{s}+J_{0}\right]-\frac{eR_{load}A}{kT}\\
J_{mp} & =\frac{kT}{eR_{load}A\left(J_{s}+J_{0}-1\right)}
\end{align*}

\end_inset

and 
\begin_inset Formula 
\[
V_{mp}=\frac{kT}{eA\left(J_{s}+J_{0}-1\right)}
\]

\end_inset

we can plug this in and get
\begin_inset Formula 
\begin{align*}
\eta_{max} & =\frac{\left[\frac{e\frac{kT}{eR_{load}A\left(J_{s}+J_{0}-1\right)}}{kT}\right]\frac{kT}{eR_{load}A\left(J_{s}+J_{0}-1\right)}J_{s}}{\left[1+\frac{e\frac{kT}{eR_{load}A\left(J_{s}+J_{0}-1\right)}}{kT}\right]N_{ph}\varepsilon_{av}}\\
 & =\frac{\left[\frac{1}{R_{load}A\left(J_{s}+J_{0}-1\right)}\right]\frac{kT}{eR_{load}A\left(J_{s}+J_{0}-1\right)}J_{s}}{\left[1+\frac{1}{R_{load}A\left(J_{s}+J_{0}-1\right)}\right]N_{ph}\varepsilon_{av}}
\end{align*}

\end_inset


\end_layout

