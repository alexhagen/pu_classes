\begin_layout Standard

\series bold
3.4) With 
\begin_inset Formula $T$
\end_inset

 measured in degrees Kelvin and 
\begin_inset Formula $k$
\end_inset

 the Boltzmann constant, the translational kinetic energy of a gas molecule
 is 
\begin_inset Formula $\frac{3}{2}kT$
\end_inset

.
 What is the corresponding de Broglie wavelength of a hydrogen atom at 
\begin_inset Formula $30\unit{\celsius}$
\end_inset

?
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E & =\frac{3}{2}kT\\
 & =\frac{3}{2}\left(8.61733\times10^{-11}\unit{\nicefrac{MeV}{K}}\right)\left(303.15\unit{K}\right)\\
 & =3.92\times10^{-8}\unit{MeV}
\end{align*}

\end_inset

and calculating the momentum, using a proton mass of 
\begin_inset Formula $938.27\unit{\nicefrac{MeV}{c^{2}}}$
\end_inset


\begin_inset Formula 
\begin{align*}
p & =\sqrt{2mE}\\
 & =\sqrt{2\left(938.27\unit{\nicefrac{MeV}{c^{2}}}\right)\left(3.92\times10^{-8}\unit{MeV}\right)}\\
 & =8.5767\times10^{-3}\unit{\nicefrac{MeV}{c}}
\end{align*}

\end_inset

and converting to wavelength
\begin_inset Formula 
\begin{align*}
\lambda & =\frac{h}{p}\\
 & =\frac{4.13566\times10^{-21}\unit{MeV\cdot s}}{8.5767\times10^{-3}\unit{\nicefrac{MeV}{c}}}\\
 & =1.4456\times10^{-10}\unit{m}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
5.9) Find the short circuit current density.
\end_layout

\begin_layout Standard
Given the following values:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
s=100\unit{\frac{cm}{s}}
\]

\end_inset


\begin_inset Formula 
\[
L=10^{-3}\unit{cm}
\]

\end_inset


\begin_inset Formula 
\[
\alpha=10^{3}\unit{\frac{1}{cm}}
\]

\end_inset


\begin_inset Formula 
\[
l=10^{-3}\unit{cm}
\]

\end_inset


\begin_inset Formula 
\[
\varepsilon_{g}=1.5\unit{eV}
\]

\end_inset


\begin_inset Formula 
\[
\phi=5\times10^{17}\unit{\frac{photons}{cm^{2}\cdot s}}
\]

\end_inset


\end_layout

\begin_layout Standard
Using equation 5-30 
\begin_inset CommandInset citation
LatexCommand cite
after "p. 192"
key "Angrist1982"

\end_inset


\begin_inset Formula 
\[
J_{s}=\eta_{co}\left(1-r\right)\left[1-\exp\left(-\alpha l\right)\right]en_{ph}
\]

\end_inset


\end_layout

\begin_layout Standard
using values from Table 5-2 
\begin_inset CommandInset citation
LatexCommand cite
after "p. 193"
key "Angrist1982"

\end_inset


\begin_inset Formula 
\[
\eta_{co}=0.61
\]

\end_inset

and calculating 
\begin_inset Formula $r$
\end_inset

 from 
\begin_inset Formula $\gamma$
\end_inset

 from Moss' relation
\begin_inset Formula 
\begin{align*}
\varepsilon_{g}\cdot\gamma^{4} & =173\\
\gamma & =\sqrt[4]{\frac{173}{\varepsilon_{g}}}\\
\gamma & =3.28
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
r & =\frac{\left(\gamma-1\right)^{2}}{\left(\gamma+1\right)^{2}}\\
 & =0.28
\end{align*}

\end_inset

and finally, calculating
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
J_{s} & =\eta_{co}\left(1-r\right)\left[1-\exp\left(-\alpha l\right)\right]en_{ph}\\
 & =\left(0.61\right)\left(1-0.28\right)\left[1-\exp\left(-\left(10^{3}\unit{\frac{1}{cm}}\right)\left(10^{-3}\unit{cm}\right)\right)\right]\left(1.6017\times10^{-19}\unit{C}\right)\left(5\times10^{17}\unit{\frac{photons}{cm^{2}s}}\right)\\
 & =0.022\unit{\frac{A}{cm^{2}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
5.11) Find the saturation current density due to holes.
\end_layout

\begin_layout Standard
Given 
\begin_inset Formula $n=10^{13}\unit{cm^{-3}}$
\end_inset

, electrons in the ntype region of 
\begin_inset Formula $n_{n}=10^{17}\unit{\frac{1}{cm^{3}}}$
\end_inset

, temperature 
\begin_inset Formula $280\unit{K}$
\end_inset

, diffusion coefficient 
\begin_inset Formula $D=81\unit{\frac{cm^{2}}{s}}$
\end_inset

 and hole lifetime of 
\begin_inset Formula $\tau=10^{-4}\unit{s}$
\end_inset

.
\end_layout

\begin_layout Standard
Calculating the hole generation current density with equation 5-37 
\begin_inset CommandInset citation
LatexCommand cite
after "p. 194"
key "Angrist1982"

\end_inset


\begin_inset Formula 
\[
J_{o\left(p\right)}=\frac{p_{n}eD_{p}}{L_{p}}
\]

\end_inset

and calculating the diffusion length with 
\begin_inset Formula 
\begin{align*}
L_{p} & =\sqrt{D_{p}\tau_{p}}\\
 & =\sqrt{\left(81\unit{\frac{cm^{2}}{s}}\right)\left(10^{-4}\unit{s}\right)}\\
 & =0.09\unit{cm}
\end{align*}

\end_inset

and finding 
\begin_inset Formula $p_{n}$
\end_inset

 from
\begin_inset Formula 
\begin{align*}
p_{n}n_{n} & =n_{i}^{2}\\
p_{n} & =\frac{n_{i}^{2}}{n_{n}}\\
 & =1\times10^{9}\unit{\frac{1}{cm^{3}}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
J_{o\left(p\right)} & =\frac{p_{n}eD_{p}}{L_{p}}\\
 & =\frac{\left(10^{9}\unit{\frac{1}{cm^{3}}}\right)\left(1.602\times10^{-19}\unit{C}\right)\left(81\unit{\frac{cm^{2}}{s}}\right)}{\left(0.09\unit{cm}\right)}\\
 & =1.44\times10^{-7}\unit{\frac{A}{cm^{2}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
5.15) a) The equilibrium hole density in the 
\begin_inset Formula $n$
\end_inset

-type region.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p_{n}=\frac{2\cdot2.23\times10^{31}\cdot300^{3}\cdot\exp\left(\frac{-0.72}{300\cdot8.617\times10^{-5}}\right)}{n_{n}}=1.11\times10^{11}\unit{\frac{1}{cm^{3}}}
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
b) the saturation current density.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
J_{0} & =\frac{p_{n}eD_{p}}{\sqrt{D_{p}\tau_{p}^{*}}}\\
 & =\frac{\left(1.11\times10^{11}\unit{\frac{1}{cm^{3}}}\right)\left(1.602\times10^{-19}\unit{C}\right)\left(44\unit{\frac{cm^{2}}{s}}\right)}{\sqrt{\left(44\unit{\frac{cm^{2}}{s}}\right)\left(1\times10^{-6}\unit{s}\right)}}\\
 & =1.1795\times10^{-4}\unit{\nicefrac{A}{cm^{2}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
c) the short circuit current density.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
J_{s} & =\frac{e\left(4\times10^{17}\right)}{4}\left[1-\exp\left(-10^{5}\cdot10^{-3}\right)\right]\\
 & =1.6\times10^{-2}\unit{\nicefrac{A}{cm^{2}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
d) the maximum power density generated by the cell under the conditions
 given in parent (c).
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\exp\left(\frac{eV_{mp}}{kT}\right)\left[1+\frac{eV_{mp}}{kT}\right] & =1+\frac{J_{s}}{J_{0}}\\
\exp\left(38.68\cdot V_{mp}\right)\left[1+38.68\cdot V_{mp}\right] & =1+\frac{1.61\times10^{-2}}{1.17\times10^{-4}}\\
V_{mp} & =0.0889\unit{V}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
J_{mp} & =\frac{38.68V_{mp}\left[J_{s}+J_{0}\right]}{\left(38.68\cdot V_{mp}+1\right)}\\
 & =0.0126\unit{\frac{A}{cm^{2}}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
P_{mp} & =J_{mp}V_{mp}\\
 & =0.011\unit{\nicefrac{W}{cm^{2}}}
\end{align*}

\end_inset


\end_layout

