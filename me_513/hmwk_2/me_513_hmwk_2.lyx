\begin_layout Paragraph
By direct substitution show that each of the following is a solution of
 the wave equation:
\end_layout

\begin_layout Subparagraph
\begin_inset Formula $f_{1}\left(x-ct\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f_{1}\left(x-ct\right)}{\partial t^{2}}=c^{2}\frac{\partial^{2}f_{1}\left(x-ct\right)}{\partial t^{2}}
\]

\end_inset

Therefore
\begin_inset Formula 
\[
\frac{\partial^{2}f_{1}\left(x-ct\right)}{\partial x^{2}}-\frac{1}{c^{2}}\left(c^{2}\frac{\partial^{2}f_{1}\left(x-ct\right)}{\partial t^{2}}\right)=0
\]

\end_inset


\end_layout

\begin_layout Subparagraph
\begin_inset Formula $\ln\left[a\left(ct-x\right)\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial\ln\left[a\left(ct-x\right)\right]}{\partial x}=-\frac{1}{ct-x}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}\ln\left[a\left(ct-x\right)\right]}{\partial x^{2}}=-\frac{1}{\left(ct-x\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\ln\left[a\left(ct-x\right)\right]}{\partial t}=\frac{c}{ct-x}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}\ln\left[a\left(ct-x\right)\right]}{\partial t^{2}}=-\frac{c^{2}}{\left(ct-x\right)^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore
\begin_inset Formula 
\begin{align*}
-\frac{1}{\left(ct-x\right)^{2}}-\frac{1}{c^{2}}\left(-\frac{c^{2}}{\left(ct-x\right)^{2}}\right) & =0\\
-\frac{1}{\left(ct-x\right)^{2}}+\frac{1}{\left(ct-x\right)^{2}} & =0\\
0 & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Subparagraph
\begin_inset Formula $a\left(ct-x\right)^{2}$
\end_inset

, and
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial a\left(ct-x\right)^{2}}{\partial x}=2a\left(x-ct\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}a\left(ct-x\right)^{2}}{\partial x^{2}}=2a
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial a\left(ct-x\right)^{2}}{\partial t}=2ac\left(ct-x\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}a\left(ct-x\right)^{2}}{\partial t^{2}}=2ac^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore
\begin_inset Formula 
\begin{align*}
2a-\frac{1}{c^{2}}\left(2ac^{2}\right) & =0\\
2a-2a & =0\\
0 & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Subparagraph
\begin_inset Formula $\cos\left[a\left(ct-x\right)\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial\cos\left[a\left(ct-x\right)\right]}{\partial x}=a\sin\left[a\left(ct-x\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}\cos\left[a\left(ct-x\right)\right]}{\partial x^{2}}=-a^{2}\cos\left[a\left(ct-x\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\cos\left[a\left(ct-x\right)\right]}{\partial t}=-ac\sin\left[a\left(ct-x\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}\cos\left[a\left(ct-x\right)\right]}{\partial t^{2}}=-a^{2}c^{2}\cos\left[a\left(ct-x\right)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore
\begin_inset Formula 
\begin{align*}
-a^{2}\cos\left[a\left(ct-x\right)\right]-\frac{1}{c^{2}}\left(-a^{2}c^{2}\cos\left[a\left(ct-x\right)\right]\right) & =0\\
-a^{2}\cos\left[a\left(ct-x\right)\right]+a^{2}\cos\left[a\left(ct-x\right)\right] & =0\\
0 & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph*
Similarly, show that each of the following is not a solution of the wave
 equation:
\end_layout

\begin_layout Subparagraph
\begin_inset Formula $a\left(ct-x^{2}\right)$
\end_inset

 and
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial a\left(ct-x^{2}\right)}{\partial x}=-2ax
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}a\left(ct-x^{2}\right)}{\partial x^{2}}=-2a
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial a\left(ct-x^{2}\right)}{\partial t}=ac
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}a\left(ct-x^{2}\right)}{\partial t^{2}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore
\begin_inset Formula 
\begin{align*}
-2a-\frac{1}{c^{2}}\left(0\right) & =0\\
-2a & \neq0
\end{align*}

\end_inset


\end_layout

\begin_layout Subparagraph
\begin_inset Formula $at\left(ct-x\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial at\left(ct-x\right)}{\partial x}=-at
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}at\left(ct-x\right)}{\partial x^{2}}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial at\left(ct-x\right)}{\partial t}=a\left(2ct-x\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial^{2}at\left(ct-x\right)}{\partial t^{2}}=2ac
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore
\begin_inset Formula 
\begin{align*}
0-\frac{1}{c^{2}}\left(2ac\right) & =0\\
-\frac{2a}{c} & \neq0
\end{align*}

\end_inset


\end_layout

